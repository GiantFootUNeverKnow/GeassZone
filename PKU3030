#include <iostream>
#include <stdio.h>

using namespace std;

int main(){
    int n;
    cin >> n;
    while (n--){
        int r, e, c;
        cin >> r >> e >> c;
        if (e > r + c)
            cout << "advertise" << endl;
        else if (e == r + c)
            cout << "does not matter" << endl;
        else
            cout << "do not advertise" << endl;
    }
    return 0;
}
